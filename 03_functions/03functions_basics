//another way
function mul(n1,n2){
    return n1*n2;

}
const result=mul(2,3)
console.log("Result :" , result)
//1
console.log(mul(2,5))
//2
 
function loggedIn(username){
    if(username === undefined){
        console.log("Please enter a username")
    }
    return `${username}   just logged in`;
}